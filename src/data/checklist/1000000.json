{
  "version": "1.0.0",
  "$id": "1000000",
  "title": "100 Pushups",
  "description": "A training program to go from just one pushup to 100 consecutive reps in less than two months.",
  "meta": {
    "author": "David Way",
    "lastUpdated": "17/01/2026",
    "tags": ["workout"]
  },
  "properties": {
    "Stage": {
      "type": "string",
      "description": "description123",
      "label": "label123",
      "enum": ["Week 1", "Week 2", "Week 3", "Week 4", "Week 5"]
    },
    "Day": {
      "type": "string",
      "enum": ["Day 1", "Day 3", "Day 5"],
      "default": "Day 1"
    },
    "Intensity": {
      "type": "string",
      "enum": ["Low", "Medium", "High"],
      "default": "Low"
    }
  },
  "required": ["Stage", "Day", "Intensity"],
  "allOf": [
    {
      "if": {
        "properties": {
          "Stage": {
            "const": "Week 1"
          },
          "Day": {
            "const": "Day 1"
          },
          "Intensity": {
            "const": "Low"
          }
        },
        "required": ["Stage", "Day", "Intensity"]
      },
      "then": {
        "properties": {
          "followUpQuestion": {
            "type": "object",
            "title": "Target Sets",
            "properties": {
              "set1": {
                "type": "boolean",
                "label": "Set 1",
                "description": "14 reps"
              },
              "set2": {
                "type": "boolean",
                "label": "Set 2",
                "description": "18 reps"
              },
              "set3": {
                "type": "boolean",
                "label": "Set 3",
                "description": "15 reps"
              },
              "set4": {
                "type": "boolean",
                "label": "Set 4",
                "description": "13 reps"
              },
              "set5": {
                "type": "boolean",
                "label": "Set 5",
                "description": "11+ reps"
              }
            }
          }
        },
        "required": ["followUpQuestion"]
      }
    }
  ]
}
